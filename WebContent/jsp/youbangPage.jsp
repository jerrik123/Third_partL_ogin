<script type="text/javascript"> 
    var scrollDelay=7;
    var Scroll=document.getElementById("ul1"); 
    var Scroll2=document.getElementById("ul2"); 
    var currentTop=0,preTop=0,stoptime=0,stopscroll=false; 
    var ScrollChild=Scroll.getElementsByTagName("li"); 
    var ScrollHeight=Scroll.offsetHeight; 
    function ScrollInfo(){ 
        if(stopscroll==true) return; 
        currentTop++; 
        if(currentTop+1>=ScrollHeight){ 
            currentTop--; 
            stoptime++; 
            if(stoptime==parseInt(ScrollHeight)*scrollDelay) { 
				currentTop=0; 
				stoptime=0; 
			}
        }else{ 
            preTop=Scroll.scrollTop; 
            //alert(preTop);
            Scroll.scrollTop++; 
            if(preTop==Scroll.scrollTop){ 
				Scroll.scrollTop=Scroll2.offsetHeight-ScrollHeight; 
				Scroll.scrollTop+=1; 
			} 
        }
    } 
    function Int_Scroll(){ 
        Scroll2.innerHTML=""; 
        Scroll2.innerHTML=Scroll.innerHTML; 
        Scroll.innerHTML=Scroll2.innerHTML+Scroll2.innerHTML; 
        Scroll.onmouseover=function(){ 
            stopscroll=true; 
        } 
        Scroll.onmouseout=function(){ 
            stopscroll=false; 
        } 
        setInterval("ScrollInfo()",scrollDelay); 
    } 
    window.setTimeout("Int_Scroll()",1000); 
</script> 
<script type="text/javascript">
	//
	function ColeeSelect(thisObj){    
	var tinput = thisObj.getElementsByTagName("input");
	var tul = thisObj.getElementsByTagName("ul");
	var tli = thisObj.getElementsByTagName("li");
	if(thisObj.id != "ColeeSelect"){
		thisObj.id = "ColeeSelect";
		thisObj.focus();
		tul[0].style.display ="";
		for(var i=0; i<tli.length; i++){
		  tli[i].onmouseover = function(){
		  	for(var j=0; j<tli.length; j++){
			tli[j].className = "";
			}
			this.className = "onlist";
		  }
		  tli[i].onclick = function(){
		  	tinput[0].value = this.innerHTML;
		  }
		}
	}else{
		ObjBlur();
	}
	thisObj.onblur = ObjBlur;
	function ObjBlur(){
		thisObj.id = "";		
		tul[0].style.display = "none";
		thisObj.blur();
	}
	}
	var Openmu = document.getElementById("openmu");
	var Opensuc = document.getElementById("suc_tj");
	//
	function changeDate(){
		if($('#birthday').val()=='yyyy-MM-dd'){
			$('#birthday').val("");
		}
	}
	function resetDate(){
		if($('#birthday').val()==''){
			$('#birthday').val("yyyy-MM-dd");
		}
	}
	function validateParams(){
		if(!$('#checkselect').attr("checked")==true){
			alert("请阅读并勾选同意活动条款和声明!");
			return false;
		}
		if($('#mobileNo').val()==''){
			alert("请填写您的手机号码!");
			return false;
		}
		if($('#userName').val()==''){
			alert("请填写您的姓名!");
			return false;
		}
		if($('#area').val()==''){
			alert("请选择您所在的地区!");
			return false;
		}
		if($('#mobileNo').val().length<8){
			alert("请正确填写您的手机号码(8-11位纯数字)!");
			return false;
		}
		if(!$('#birthday').val().match(/^(\d{4})(-|\/)(\d{1,2})\2(\d{1,2})$/)){
			alert("请输入正确的日期格式yyyy-MM-dd!");
			return false;
		}
		return true;
	}
	function openbox(){		
		$("#submitbutton").attr("disabled",true);
		if(!validateParams()){
			$("#submitbutton").attr("disabled",false);
			return false;
		}
		var birthday = '';
		if($('#birthday').val()!='yyyy-MM-dd'){
			birthday = $('#birthday').val();
		}
		var url = "<%=request.getContextPath()%>/youbang/youbangRegister.action";
		var params = {'mobileNo':$('#mobileNo').val(),'userName':$('#userName').val(),'area':$('#area').val(),'birthday':birthday};
		$.post(url,params,function(data){
			if(data.result=="1"){
				alert("您输入的信息不完整或信息不合法，请重新输入!");
			}else if(data.result=="2"){
				alert("您输入的手机号已存在,请检查手机号是否正确!");
			}else if(data.result=="0"){
				Openmu.style.display ="block";
				Opensuc.style.display ="block";
			}else{
				alert("系统异常,请重试!");
			}
			$("#submitbutton").attr("disabled",false);
		},"json");
	}
	function closebox(){
		Openmu.style.display ="none";
		Opensuc.style.display ="none";
	}
</script>
