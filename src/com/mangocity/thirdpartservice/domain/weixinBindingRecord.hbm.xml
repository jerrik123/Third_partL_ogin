<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.mangocity.thirdpartservice.domain.WeixinBindingInfo" table="T_MBR_THIRD_WEIXIN_BINDING">
		<id name="id" type="java.lang.Long" column="ID">
			<generator class="sequence">
				<param name="sequence">seq_mbr_third_weixin_binding</param>
			</generator>
		</id>
		<property name="mbrId" column="MBR_ID" type="java.lang.Long"/>
		<property name="mobile" column="MOBILE" type="string"></property>
		<property name="openId" column="OPENID" type="string"></property>
		<property name="createTime" column="CREATE_TIME" type="timestamp"></property>
		<property name="updateTime" column="UPDATE_TIME" type="timestamp"></property>
		<property name="status" column="STATUS" type="java.lang.Integer"></property>
	</class>
</hibernate-mapping>
